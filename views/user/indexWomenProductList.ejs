  <%- include('../partial/index-partial/_navigation.ejs')%>
  <div class="col-md-6">
    <div class="paragraph">
    <h1>
        <span class="text" >Finest</span>
    </h1>
    <h6><span class="text1">WEDDING COLLECTION</span></h6>
    </div>
    <div class="paragraph">
                <h3 class="colorheading">Bridal Lehengas</h3>
